@model IEnumerable<OnDemandService.Models.Pincode>

@{
    ViewBag.Title = "Index";
    Layout = "~/Areas/Admin/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table table-striped">
    <tr>
        <th>
            Pincode
        </th>
        <th>
            Latitude
        </th>
        <th>
            Longitude
        </th>
        <th>
            City
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Pincode1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Latitude)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Longitude)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.City.CityName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.PincodeId }) |
            @Html.ActionLink("Details", "Details", new { id=item.PincodeId }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.PincodeId })
        </td>
    </tr>
}

</table>
